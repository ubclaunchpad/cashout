<h1>Snapshot</h1>

<table class="table" style="margin-top: 10px;">
    <thead>
        <tr>
            <th>Currency</th>
            <th>Value</th>
            <th colspan=3></th>
        </tr>
    </thead>

    <tbody>
        <% @snapshot_data.each do |currency, value| %>
        <tr>
            <td><%= currency %></td>
            <td><%= value.round(4) %></td>
        </tr>
        <% end %>
    </tbody>
</table>
